# Frontend Environment Variables
# Copy this file to .env and fill in your actual values

# API Configuration
VITE_API_URL=http://localhost:5001
VITE_BACKEND_URL=http://localhost:54112

# Firebase Configuration
VITE_FIREBASE_API_KEY=your_firebase_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=123456789
VITE_FIREBASE_APP_ID=1:123456789:web:abcdef123456

# Analytics & Monitoring
VITE_AMPLITUDE_API_KEY=your_amplitude_key_here
VITE_SENTRY_DSN=your_sentry_dsn_here
VITE_VERCEL_ANALYTICS_ID=your_vercel_analytics_id

# Feature Flags
VITE_ENABLE_DEBUG_MODE=false
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_REPORTING=true

# Development
VITE_DEV_MODE=true
VITE_MOCK_API=false

# Security
VITE_ENCRYPTION_KEY=your_32_char_encryption_key_here

# Third-party Services
VITE_CLOUDINARY_CLOUD_NAME=your_cloudinary_name
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_key_here

# Performance
VITE_ENABLE_SERVICE_WORKER=true
VITE_CACHE_VERSION=v1.0.0
VITE_ENABLE_PERFORMANCE_MONITORING=true
VITE_PERFORMANCE_SAMPLE_RATE=0.1
VITE_ENABLE_WEB_VITALS=true
