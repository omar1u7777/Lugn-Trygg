{
  "name": "lugn-trygg-frontend",
  "version": "1.0.0",
  "description": "Lugn & Trygg - En säker och användarvänlig desktop-applikation för välmående och trygghet.",
  "author": "Lugn & Trygg Development Team",
  "license": "MIT",
  "type": "module",
  "private": true,
  "main": "main.cjs",
  "homepage": "./",
  "proxy": "http://localhost:54112",
  "scripts": {
    "dev": "concurrently \"npx vite\" \"npx electron .\"",
    "build": "npx vite build",
    "build:electron": "npm run build && electron-builder",
    "build:electron:win": "npm run build && electron-builder --win",
    "build:electron:mac": "npm run build && electron-builder --mac",
    "build:electron:linux": "npm run build && electron-builder --linux",
    "start": "npx vite preview",
    "serve": "npx vite preview --host 0.0.0.0 --port 3000",
    "lint": "eslint . --fix",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "chart.js": "^4.5.0",
    "cloudinary": "^1.41.3",
    "crypto-js": "^4.2.0",
    "electron-store": "^9.0.0",
    "express-session": "^1.17.3",
    "firebase": "^10.11.1",
    "firebase-admin": "^11.10.0",
    "framer-motion": "^12.23.24",
    "i18next": "^25.5.3",
    "i18next-browser-languagedetector": "^8.2.0",
    "js-cookie": "^3.0.5",
    "jsonwebtoken": "^9.0.0",
    "jwt-decode": "^4.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "msw": "^2.7.0",
    "next": "^14.2.0",
    "punycode": "^2.3.1",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^16.0.0",
    "react-router-dom": "^7.2.0"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.14.6",
    "@electron/remote": "^2.1.2",
    "@eslint/js": "^9.37.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/js-cookie": "^3.0.6",
    "@types/jwt-decode": "^2.2.1",
    "@types/lodash": "^4.17.20",
    "@types/next": "^8.0.7",
    "@types/node": "^22.13.8",
    "@types/punycode": "^2.1.4",
    "@types/react": "^18.3.25",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "@vitejs/plugin-react": "^5.0.4",
    "concurrently": "^8.2.0",
    "cypress": "^15.3.0",
    "cypress-axe": "^1.7.0",
    "electron": "^28.3.3",
    "electron-builder": "^24.13.3",
    "electron-updater": "^6.3.9",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.23",
    "globals": "^16.4.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.2.4",
    "typescript": "^5.2.2",
    "typescript-eslint": "^8.45.0",
    "vite": "^5.4.20"
  },
  "build": {
    "appId": "com.lugn.trygg",
    "productName": "Lugn & Trygg",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "package.json",
      "main.cjs",
      "preload.js"
    ],
    "extraResources": [
      {
        "from": "main.cjs",
        "to": "app/main.cjs"
      }
    ],
    "win": {
      "target": "nsis",
      "icon": "public/icon.png"
    },
    "mac": {
      "target": "dmg",
      "icon": "public/icon.icns"
    },
    "linux": {
      "target": "AppImage",
      "icon": "public/icon.png"
    }
  }
}
